// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Prompt {
  id        String   @id @default(cuid())
  content   String
  model1    String
  model2    String
  response1 String?
  response2 String?
  cost1     Float?
  cost2     Float?
  createdAt DateTime @default(now())
}
